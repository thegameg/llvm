# RUN: llc -mtriple=x86_64-- -run-pass=shrink-wrap2 %s -enable-shrink-wrap2=true -debug-only=shrink-wrap2 -o /dev/null 2>&1 | FileCheck %s
# REQUIRE: asserts
--- |
  define void @fun() nounwind {
  entry:
    ret void
  }
...
---
name:            fun
tracksRegLiveness: true
body:             |
  bb.0:
    successors: %bb.1, %bb.4

    NOOP implicit-def %eflags
    JE_1 %bb.1, implicit killed %eflags
    JMP_1 %bb.4

  bb.1:
    successors: %bb.2, %bb.5

    NOOP implicit-def %eflags
    JE_1 %bb.2, implicit killed %eflags
    JMP_1 %bb.5

  bb.2:
    successors: %bb.3

    %rbx = MOV64ri 30
    JMP_1 %bb.3

  bb.3:
    successors: %bb.3, %bb.5

    NOOP implicit-def %eflags
    JE_1 %bb.3, implicit killed %eflags
    JMP_1 %bb.5

  bb.5:
    successors: %bb.5, %bb.6

    NOOP implicit-def %eflags
    JE_1 %bb.5, implicit killed %eflags
    JMP_1 %bb.6

  bb.4:
    RET 0

  bb.6:
    RET 0

...
#CHECK-LABEL: fun

#CHECK: BB#2 uses : %RBX
#CHECK: **** Shrink-wrapping results
#CHECK-NEXT: BB#2: Saves: %RBX, | Restores: %RBX
